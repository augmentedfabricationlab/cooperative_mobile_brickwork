                    plane.Translate(plane.XAxis*0.025/i)
                    plane.Rotate(m.radians(0), plane.ZAxis)
                elif i &gt; len(planes)-8:
                    plane.Translate(plane.XAxis*0.0/i)
                    plane.Rotate(m.radians(0), plane.ZAxis)
                    
                row.append(plane)
                
