        #rotval2 = math.sin(i/6)*rot_factor2 #"rot_factor2" used to be "rot_factor"; introduce new parameter to create layer different rotation angle

        #---Absolute rotation function---
        #rotval1 = i/16*rot_factor1
        rotval2 = -rot_factor2
        new_plane= rg.Plane(plane)
        
        if j%2==0: #even layers
            new_plane.Rotate(-math.radians(rotval1), plane.ZAxis, plane.Origin)
        else:
            new_plane.Rotate(math.radians(rotval2), plane.ZAxis, plane.Origin)
        
