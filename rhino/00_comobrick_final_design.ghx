                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
import math as m
import rhinoscriptsyntax as rs
from Rhino.Geometry import CurveOffsetCornerStyle as cs

#new_curve = input_crv.Trim(0.1, 0.2)

start_pts = []
end_pts = []
curves = []
offset_curves = []

def list_from_2darray(array):
    return [item for sublist in array for item in sublist]
    
def create_curves(input_crv, angle1, angle2, interval, num):

    offset = 0
    
    for i in range(num):
        # duplicate and translate a new curve
        new_curve = rg.Curve.DuplicateCurve(input_crv)
        new_curve.Translate(rg.Vector3d.ZAxis*interval*i)
        
        # get the plane on which the new curve lies
        #crv_plane = rg.Plane(new_curve.PointAtStart, new_curve.PointAtEnd, new_curve.PointAtNormalizedLength(0.5))
        crv_plane = rg.Plane(new_curve.PointAtStart, rg.Vector3d.ZAxis)
        # compute an offset value based on layer number
        if i &lt; before_layer_num:
            offset += offset_step
        elif i &gt; after_layer_num:
            offset -= offset_step
        
        # offset the new curve
        offset_crv = rg.Curve.Offset(new_curve, crv_plane, -offset, 0.01, cs.None)[0]
        
        # for visualization
        offset_curves.append(offset_crv)
        
        # rotate the new curve around the axis defined by its end points (angle1); 
        # add an additional rotation of the curve based on layer number (angle2*i)
        rot_vector = rg.Vector3d(input_crv.PointAtStart) - rg.Vector3d(input_crv.PointAtEnd)
        offset_crv.Rotate(m.radians(-angle1-(i*angle2)), rot_vector, input_crv.PointAtNormalizedLength(0.5))
        
        # add the new curve to the list of curves
        curves.append(offset_crv)
    return curves

def create_planegrid(input_crv, angle1, angle2, interval, num, bricklen):

    curves = create_curves(input_crv, angle1, angle2, interval, num)
    
    plane_grid = []
    offset = 0
    for j, c in enumerate(curves):
        
        #new_start_pt = c.PointAtNormalizedLength(value_list[j])
        new_start_pt = c.PointAtNormalizedLength(0.04*(j/10.))
        c.SetStartPoint(new_start_pt)
    
        row = []

        #vparams = rg.Curve.DivideByCount(c, cols, True)
        vparams = rg.Curve.DivideByLength(c, 0.1325, True)
        planes = rg.Curve.GetPerpendicularFrames(c, vparams)

        for i, plane in enumerate(planes):
            
            plane.Rotate(m.radians(-90), plane.YAxis)
            plane.Rotate(m.radians(-90), plane.XAxis)

            if j%2 == 0 and i%2 == 0: #even course
                if i &lt; 9:
                    plane.Translate(plane.XAxis*+0.0/i) # 0.0
                    plane.Rotate(m.radians(0), plane.ZAxis)
                elif i &gt; len(planes)-11:
                    plane.Translate(plane.XAxis*-0.2/i) # 0.1
                    plane.Rotate(m.radians(0), plane.ZAxis)
                    
                row.append(plane)
                
            if j%2 == 1 and i%2 == 1: #odd course
                if i &lt; 9:
                    plane.Translate(plane.XAxis*+0.01/i) # 0.02
                elif i &gt; len(planes)-8:
                    plane.Translate(plane.XAxis*-0.4/i*(i/50)) # 0.1

                row.append(plane)

        plane_grid.append(row)
    return plane_grid

plane_grid = create_planegrid(input_crv, angle1, angle2, interval, num, bricklen)
