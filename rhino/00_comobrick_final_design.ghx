                    <item name="CodeInput" type_name="gh_string" type_code="10">from scriptcontext import sticky as st
from compas_ghpython import update_component


L = list(assembly.elements())
# store data in component
guid = str(ghenv.Component.InstanceGuid)
counter_key = "counter_" + guid
running_key = "running_" + guid

if counter_key not in st:
    st[counter_key] = 0

if running_key not in st:
    st[running_key] = False
    

if st[running_key]:
    if st[counter_key] &lt; len(L) - 1:
        st[counter_key] += 1
        update_component(ghenv, interval)
    else:
        st[counter_key] = 0
        st[running_key] = False
    
    
if start:
    st[running_key] = True

if stop:
    st[running_key] = False

if step:
    if st[counter_key] &lt; len(L) - 1:
        st[counter_key] += 1

if step_back:
    if  0 &lt; st[counter_key] &lt; len(L):
        st[counter_key] -= 1

if reset:
    st[counter_key] = 0

if till_key:
    st[counter_key] = key

if full:
    st[counter_key] = len(L)-1

if len(L):
    item = L[st[counter_key]]



#st["visualisation_counter"] = st[counter_key]
#st[counter_key]= 500
counter = st[counter_key]</item>
