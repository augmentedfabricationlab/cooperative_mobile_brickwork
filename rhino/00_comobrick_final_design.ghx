            plane.Translate(plane.YAxis*-offset)

            if j%2 == 0 and i%2 == 0: #even course
                if i==0:
                    #plane.Translate(plane.XAxis*-bricklen/4)
                    #plane.Translate(plane.YAxis*-bricklen/6)
                    plane.Rotate(m.radians(0), plane.ZAxis)
                elif i==len(planes)-1:
                    #plane.Translate(plane.XAxis*bricklen/4)
