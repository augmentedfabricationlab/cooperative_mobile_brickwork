                    <item name="CodeInput" type_name="gh_string" type_code="10">from assembly_information_model.assembly.courses import assembly_courses, assembly_with_interfaces_courses


if compute_interfaces_numpy:
    import os
    from compas.rpc import Proxy
    from assembly_information_model.assembly import Assembly
    p = Proxy('cooperative_mobile_brickwork.assembly.interfaces_numpy')
    p.stop_server()
    p.start_server()

    filename_before = "brickwall_ex2.json"
    filename_after = "brickwall_interfaces_ex2.json"

    PATH_BEFORE = os.path.join(DATA, filename_before)
    PATH_AFTER = os.path.join(DATA, filename_after)
    
    # the assembly has to be serialized such that it can be called from CPython
    assembly.to_json(PATH_BEFORE)
    
    # call the CPython Function using COMPAS RPC
    p.assembly_interfaces_numpy_fromtojson(PATH_BEFORE, PATH_AFTER, nmax=10, tmax= 0.04, amin = 0.00001)

    assembly = Assembly.from_json(PATH_AFTER)


# now compute the courses
