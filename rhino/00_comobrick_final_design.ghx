                    <item name="CodeInput" type_name="gh_string" type_code="10">import os
import sys

import math

from compas.colors import Color
from compas.artists import Artist


G = []
color = Color.from_rgb255(243,169,137)

key_index = assembly.network.key_index()
keys = [key_index[key] for key in assembly.network.nodes()]
elements = [assembly.element(key) for key in keys]
#print elements
#print([(e.gripping_frame) for e in elements])

tool_planes_WCS = [Artist(e.tool_frame).draw() for e in elements]

current_tool_plane = tool_planes_WCS[counter]

if show_all and not show_counter:
    for element in elements:
        mesh = Artist(element.mesh).draw(color)
        G.append(mesh)

if show_counter:
    for element in elements[0:counter+1]:
        mesh = Artist(element.mesh).draw(color)
        G.append(mesh)




