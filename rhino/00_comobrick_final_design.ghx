import Rhino.Geometry as rg


assembly = Assembly() # initialize an empty assembly class

current_course = 0
for j, row in enumerate(plane_grid):
    for i, plane in enumerate(row):
        
        new_plane = rg.Plane(plane)
        
        if rot_mode == 1:
            rotval1 = math.sin(i/8)*rot_factor1
            #rotval2 = math.sin(i/6)*rot_factor2 
            rotval2 = -rot_factor2
        
        if rot_mode == 2:
            min_dist = 1.50
            for attr_pt in attr_pts:
                dist =  attr_pt.DistanceTo(new_plane.Origin)
                #print dist
                if dist &lt; min_dist: 
                    min_dist = dist
            dist = min_dist
            
            rot_val = (dist/10)*0.4
            angle = math.radians(rot_val**2)*5000

#            if i &lt; 2:
#               angle = 0

        #print 'course:', j, 'brick:', i, 'angle: ' , math.degrees(angle)
        
        if j%2==0: #even layers
            new_plane.Rotate(angle, plane.ZAxis, plane.Origin)
        else:
            new_plane.Rotate(-angle, plane.ZAxis, plane.Origin)
        
        T = Transformation.from_frame_to_frame(Frame.worldXY(), Frame(new_plane.Origin, new_plane.XAxis, new_plane.YAxis))
        
        
        if plane.OriginZ &gt; 0.35:
        #if True:
            e = elem_full.transformed(T)
            assembly.add_element(e)


if transform:
