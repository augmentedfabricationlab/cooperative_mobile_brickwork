def create_curves(input_crv, angle1, angle2, interval, num):

    offset = 0
    
    for i in range(num):
        # duplicate and translate a new curve
        new_curve = rg.Curve.DuplicateCurve(input_crv)
        new_curve.Translate(rg.Vector3d.ZAxis*interval*i)
        
        # get the plane on which the new curve lies
        #crv_plane = rg.Plane(new_curve.PointAtStart, new_curve.PointAtEnd, new_curve.PointAtNormalizedLength(0.5))
        crv_plane = rg.Plane(new_curve.PointAtStart, rg.Vector3d.ZAxis)
        # compute an offset value based on layer number
        if i &lt; before_layer_num:
            offset += offset_step
        elif i &gt; after_layer_num:
            offset -= offset_step
        
        # offset the new curve
        offset_crv = rg.Curve.Offset(new_curve, crv_plane, -offset, 0.01, cs.Sharp)[0]
        
        # for visualization
        offset_curves.append(offset_crv)
        
        # rotate the new curve around the axis defined by its end points (angle1); 
        # add an additional rotation of the curve based on layer number (angle2*i)
        rot_vector = rg.Vector3d(input_crv.PointAtStart) - rg.Vector3d(input_crv.PointAtEnd)
        offset_crv.Rotate(m.radians(-angle1-(i*angle2)), rot_vector, input_crv.PointAtNormalizedLength(0.5))
        
        # add the new curve to the list of curves
        curves.append(offset_crv)
    return curves

